{
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "lib": [ "esnext", "DOM", "DOM.Iterable" ],
    "target": "es2019",
    "plugins": [
      "name": "ts-lit-plugin"
    ]
  }
}

